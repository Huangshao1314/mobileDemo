{
  "name": "element-demo",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://huangshao1314.github.io/element-demo/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "new:comp": "node ./tools/generate-component",
    "new:view": "node ./tools/generate-view",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e -- mode development --headless",
    "deploy": "sh deploy.sh",
    "analyz": "vue-cli-service build --mode analyz",
    "build:development": "vue-cli-service build --mode development",
    "build:pex": "vue-cli-service build --mode productionpex",
    "build:ppt": "vue-cli-service build --mode productionppt",
    "build:sit": "vue-cli-service build --mode sit",
    "build:staging": "vue-cli-service build --mode staging",
    "prepare": "cp-cli example.env.local .env.local",
    "serve:build": "npm run build && serve -s dist",
    "svgo": "svgo -f src/components/svg-icon/svgs --config=src/components/svg-icon/svgo.yml",
    "test:e2e-gui": "vue-cli-service test:e2e --mode development"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "axios": "^0.19.0",
    "big.js": "^5.2.2",
    "cookiejs": "^1.0.25",
    "core-js": "^2.6.5",
    "cp-cli": "^2.0.0",
    "dayjs": "^1.8.24",
    "driver.js": "^0.9.8",
    "element-ui": "^2.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eventemitter3": "^4.0.0",
    "is-string": "^1.0.5",
    "js-cookie": "^2.2.0",
    "lint-staged": "^8.1.7",
    "lodash": "^4.17.11",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.1",
    "postcss-advanced-variables": "^3.0.0",
    "postcss-partial-import": "^4.1.0",
    "precss": "^4.0.0",
    "qs": "^6.9.3",
    "query-string": "^6.12.1",
    "raphael": "^2.3.0",
    "sockjs-client": "^1.4.0",
    "sortablejs": "^1.10.2",
    "svg-sprite-loader": "^4.1.6",
    "vue": "^2.6.10",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.15.4",
    "vue-router": "^3.0.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.3.0",
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.2",
    "@vue/cli-service": "~4.2.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
